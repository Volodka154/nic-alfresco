							<control-param name="field">btl-document:regNumber,btl-document:regDate,btl-document:content</control-param>
							<control-param name="s_field">btl-document:regNumber</control-param>
							<control-param name="type">btl-incomming:documentDataType,btl-internal:documentDataType</control-param>
							<control-param name="style">width:225px; height:20px; padding: 6px 12px; </control-param>
							<control-param name="link">true</control-param>
						</control>
					</field>

					<field id="btl-document:additionTo">
						<control template="/ru/btl/fpi/components/form/controls/btl_associationMany.ftl">	
						   <control-param name="field">btl-document:regNumber,btl-document:regDate</control-param>
						   <control-param name="s_field">btl-document:regNumber</control-param>
							<control-param name="type">btl-internal:documentDataType</control-param>							
						   <control-param name="style">width:225px; height:20px; padding: 6px 12px; </control-param>	
						   <control-param name="link">true</control-param>		       
					    </control>					
                    </field> 													
							
				</appearance>
			
			</form>
		</forms>
	</config>
	
