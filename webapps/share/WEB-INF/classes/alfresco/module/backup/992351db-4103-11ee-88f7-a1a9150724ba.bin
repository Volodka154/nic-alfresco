						 <control template="/org/alfresco/components/form/controls/textarea.ftl">   
						 	<control-param name="rows">10</control-param>	
						 	<control-param name="style">border-radius: 4px; resize: none; border-color: red;</control-param>
						 	<control-param name="noLabel">true</control-param>	
