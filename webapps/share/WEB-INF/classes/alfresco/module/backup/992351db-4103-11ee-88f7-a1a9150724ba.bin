							<control-param name="style">width:110px; height:20px; padding: 6px 12px; border-color: red;</control-param>	
							
							<control-param name="filter">type:%27%5Cu0412%5Cu043D%5Cu0443%5Cu0442%5Cu0440%5Cu0435%5Cu043D%5Cu043D%5Cu0438%5Cu0439%27</control-param>
						</control>					
