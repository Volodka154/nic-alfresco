							<control-param name="s_field">shortName</control-param> 	
							<control-param name="activelink">true</control-param> 
							<control-param name="type">btl-project:projectDataType</control-param>						
							<control-param name="style">width:520px; height:20px; padding: 6px 12px; </control-param>	
							
						</control>						
